{
  "name": "Japanese Flashcards",
  "description": "Pops up flashcards at times to help you learn Japanese words.",
  "version": "0.31",
  "icons": {
    "16": "images/logo-32.png",
    "32": "images/logo-32.png",
    "48": "images/logo-48.png",
    "128": "images/logo-128.png"
  },
  "background": {
    "page": "background.html"
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": {
      "38": "images/logo-plain-256.png"
    },
    "default_title": "Japanese Flashcards",
    "default_popup": "options.html"
  },
  "permissions": [
    "tabs",
    "notifications",
    "http://*/*",
    "https://*/*"
  ],
  "manifest_version": 2,
  "web_accessible_resources": [
    "flashcards.json"
  ]
}
